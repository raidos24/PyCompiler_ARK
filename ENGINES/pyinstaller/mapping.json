    "clean": {"flag": "--clean", "type": "bool"},
    "noupx": {"flag": "--noupx", "type": "bool"},
    "debug": {"flag": "--debug", "type": "bool"},
    "icon": {"flag": "--icon", "type": "value"},
    "distpath": {"flag": "--distpath", "type": "value"},
    "add_data": {"flag": "--add-data", "type": "multi_pair", "sep": ";"}
  }
